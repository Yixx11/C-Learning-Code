<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800" Loaded="Window_Loaded">
    <Grid Margin="0,0,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>

        <Grid x:Name="mapGrid" Background="#FF575757" Grid.Column="2">
        </Grid>
        <Button x:Name="width" Content="WidthFirst" HorizontalAlignment="Left" Margin="34,174,0,0" VerticalAlignment="Top" Height="56" Width="115" Click="width_Click"/>
        <Button x:Name="deepth" Content="DeepthFirst" HorizontalAlignment="Left" Margin="34,389,0,0" VerticalAlignment="Top" Height="56" Width="115" Click="deepth_Click"/>
        <TextBox x:Name="variable" HorizontalAlignment="Left" Margin="34,100,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
